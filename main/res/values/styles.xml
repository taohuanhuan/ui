<resources>

    <style name="B5_Font">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">#ff0e0e0e</item>
        <item name="android:shadowColor">@color/semi_transparent_w</item>
        <item name="android:shadowDy">-1.0</item>
        <item name="android:shadowRadius">0.2</item>
    </style>

    <style name="G1_Font">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textColor">#ffeeeeee</item>
    </style>

    <style name="ThemeActivity" mce_bogus="1">
        <item name="android:windowAnimationStyle">@style/AnimationActivity</item>
        <item name="android:windowNoTitle">true</item>
    </style>
    <!-- activity动画 -->
    <style name="AnimationActivity" mce_bogus="1" parent="@android:style/Theme.Black.NoTitleBar.Fullscreen">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:activityOpenEnterAnimation">@anim/push_left_in</item>
        <item name="android:activityOpenExitAnimation">@anim/push_left_out</item>
        <item name="android:activityCloseEnterAnimation">@anim/push_right_in</item>
        <item name="android:activityCloseExitAnimation">@anim/push_right_out</item>
        <item name="android:taskOpenEnterAnimation">@anim/push_left_in</item>
        <item name="android:taskOpenExitAnimation">@anim/push_left_out</item>
        <item name="android:taskCloseEnterAnimation">@anim/push_right_in</item>
        <item name="android:taskCloseExitAnimation">@anim/push_right_out</item>
        <item name="android:taskToFrontEnterAnimation">@anim/push_left_in</item>
        <item name="android:taskToFrontExitAnimation">@anim/push_left_out</item>
        <item name="android:taskToBackEnterAnimation">@anim/push_right_in</item>
        <item name="android:taskToBackExitAnimation">@anim/push_right_out</item>
    </style>
    <!--首页底部按钮-->
    <style name="main_bottom_btn_b">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">11sp</item>
        <item name="android:paddingTop">5dip</item>
        <item name="android:paddingBottom">5dip</item>
        <item name="android:background">@color/no_color</item>
        <item name="android:textColor">@color/main_bottom_text</item>
        <item name="android:button">@null</item>
    </style>
    <!--首页顶部中间文字-->
    <style name="main_top_middle_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:textColor">@color/iuv_txt_title</item>
        <item name="android:textSize">@dimen/top_content_text</item>
    </style>
    <!--首页左边中间按钮-->
    <style name="main_top_left_button">
        <item name="android:layout_width">65dp</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">@dimen/top_left_margin</item>
        <item name="android:background">@drawable/top_left_saotu</item>

    </style>
    <!--首页顶部右边按钮-->
    <style name="main_top_right_button">
        <item name="android:layout_width">30dp</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginRight">@dimen/top_right_margin</item>
        <item name="android:background">@drawable/top_right_selector</item>

    </style>

    <style name="main_bottom_btn_a">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">bottom</item>
        <item name="android:background">@color/no_color</item>
        <item name="android:textColor">@color/main_bottom_text</item>
        <item name="android:button">@null</item>
    </style>

    <style name="main_bottom_btn_c">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">14sp</item>
        <item name="android:paddingTop">5dip</item>
        <item name="android:paddingBottom">5dip</item>
        <item name="android:background">@color/no_color</item>
        <item name="android:textColor">@color/main_bottom_text</item>
        <item name="android:button">@null</item>
    </style>

    <style name="main_bottom_btn_c2">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_weight">4</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">14sp</item>
        <item name="android:paddingTop">5dip</item>
        <item name="android:paddingBottom">5dip</item>
        <item name="android:background">@color/no_color</item>
        <item name="android:textColor">@color/main_bottom_text</item>
        <item name="android:button">@null</item>
    </style>

    <style name="main_bottom_btn_c1">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">wrap_content</item>


        <item name="android:background">@color/no_color</item>
        <item name="android:textColor">@color/main_bottom_text</item>
        <item name="android:button">@null</item>
    </style>
    <!--登录 -->
    <style name="login">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">bottom</item>
        <item name="android:background">@drawable/login_selected</item>
        <item name="android:textColor">@color/shape_login_text</item>
        <item name="android:button">@null</item>
    </style>
    <!--公共弹出框-->
    <style name="delete_pop_style" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">@color/no_color</item>
        <item name="android:windowBackground">@color/no_color</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:windowAnimationStyle">@style/delete_pop_anim</item>
    </style>

    <style name="delete_pop_anim" parent="@android:style/Animation.Dialog">
        <item name="android:windowEnterAnimation">@anim/push_up_in</item>
        <item name="android:windowExitAnimation">@anim/push_up_out</item>
    </style>

    <!-- AndroidImageSlider theme start-->
    <style name="AndroidImageSlider_Corner_Oval_Orange">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/indicator_corner_bg</item>
        <item name="android:paddingLeft">2dp</item>
        <item name="android:paddingRight">2dp</item>
        <item name="shape">oval</item>
        <item name="padding_left">3dp</item>
        <item name="padding_right">3dp</item>
        <item name="padding_top">4dp</item>
        <item name="padding_bottom">4dp</item>

        <item name="selected_color">#f60</item>
        <item name="unselected_color">#ffffff</item>
        <item name="selected_width">6dp</item>
        <item name="selected_height">6dp</item>
        <item name="unselected_width">6dp</item>
        <item name="unselected_height">6dp</item>
    </style>

    <style name="AndroidImageSlider_Attractive_Rect_Blue">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingLeft">2dp</item>
        <item name="android:paddingRight">2dp</item>
        <item name="shape">rect</item>
        <item name="padding_left">3dp</item>
        <item name="padding_right">3dp</item>
        <item name="padding_top">4dp</item>
        <item name="padding_bottom">4dp</item>
        <item name="selected_color">#30CCE5</item>
        <item name="unselected_color">#333333</item>
        <item name="selected_width">13dp</item>
        <item name="selected_height">4dp</item>
        <item name="unselected_width">13dp</item>
        <item name="unselected_height">4dp</item>
    </style>

    <style name="AndroidImageSlider_Magnifier_Oval_Black">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingLeft">2dp</item>
        <item name="android:paddingRight">2dp</item>
        <item name="shape">oval</item>
        <item name="padding_left">3dp</item>
        <item name="padding_right">3dp</item>
        <item name="padding_top">4dp</item>
        <item name="padding_bottom">4dp</item>
        <item name="selected_color">#333333</item>
        <item name="unselected_color">#33000000</item>
        <item name="selected_width">8dp</item>
        <item name="selected_height">8dp</item>
        <item name="unselected_width">6dp</item>
        <item name="unselected_height">6dp</item>
    </style>
    <!-- AndroidImageSlider theme end-->
    <style name="market_category_radiobtn">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:textSize">18sp</item>
        <item name="android:paddingTop">8dip</item>
        <item name="android:paddingBottom">4dip</item>
        <item name="android:textColor">@color/market_radiobtn_text</item>
        <item name="android:button">@null</item>
    </style>
    <!--个人中心一行的RelativeLayout属性-->
    <style name="layout4_content_item">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/linear_select</item>
        <item name="android:clickable">true</item>
        <item name="android:paddingBottom">13dp</item>"
        <item name="android:paddingLeft">25dp</item>
        <item name="android:paddingRight">22dp</item>"
        <item name="android:paddingTop">13dp</item>
    </style>

    <style name="layout4_content_item_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textColor">@color/item_text</item>
        <item name="android:textSize">@dimen/layout4_item_text</item>
    </style>

    <style name="layout4_content_item_image">
        <item name="android:layout_width">25dp</item>>
        <item name="android:layout_height">25dp</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

</resources>
